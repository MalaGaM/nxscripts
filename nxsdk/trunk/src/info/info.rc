    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "FileDescription",  "Display information about ioFTPD."
            VALUE "FileVersion",      VERSION
            VALUE "InternalName",     "nxSDK - Info"
            VALUE "LegalCopyright",   "Copyright (c) 2006 neoxed"
            VALUE "OriginalFilename", "info.exe"
            VALUE "ProductName",      "nxSDK - Info"
            VALUE "ProductVersion",   VERSION
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
